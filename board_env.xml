<?xml version="1.0"?>
<!-- 
  DE10 Agilex oneAPI ASP Board Environment
  Based on OFS 2024.2-1 with OPAE runtime
-->
<board_env version="2024.0" name="de10-agilex">
  <hardware dir="hardware" default="ofs_de10_agilex"></hardware>
  <platform name="linux64">
    <mmdlib>libopae-c.so,%b/linux64/lib/libMPF.so,%b/linux64/lib/libintel_opae_mmd.so</mmdlib>
    <linkflags>-L%a/linux64/lib -L%b/linux64/lib</linkflags>
    <linklibs>-lintel_opae_mmd -lrt -lMPF</linklibs>
    <utilbindir>%b/linux64/libexec</utilbindir>
  </platform>
</board_env>
